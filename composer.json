{
    "name": "equidna/swift-auth",
    "description": "Bottled authentication for Laravel projects",
    "version": "1.0.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Ruelas",
            "email": "gruelas@gruelas.com"
        },
        {
            "name": "Raul Cruz",
            "email": "rcruz@teleurban.mx"
        },
        {
            "name": "Guillermo Corona",
            "email": "gcorona@teleurban.mx"
        }
    ],
    "require": {
        "php": "^8.2",
        "equidna/bird-flock": "^1.0",
        "equidna/laravel-toolkit": "*",
        "illuminate/support": "^11.21 || ^12.0",
        "inertiajs/inertia-laravel": "^1.0",
        "laravel/framework": "^11.21 || ^12.0",
        "laravel/helpers": "^1.7"
    },
    "require-dev": {
        "larastan/larastan": "^3.8",
        "phpunit/phpunit": "^12.4",
        "squizlabs/php_codesniffer": "*",
        "stevebauman/autodoc-facades": "*"
    },
    "autoload": {
        "psr-4": {
            "Equidna\\SwiftAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Equidna\\SwiftAuth\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": []
    },
    "extra": {
        "laravel": {
            "providers": [
                "Equidna\\SwiftAuth\\Providers\\SwiftAuthServiceProvider"
            ]
        }
    },
    "repositories": {},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
