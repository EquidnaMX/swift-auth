{
    "name": "equidna/swift-auth",
    "description": "Bottled authentication for Laravel projects",
    "version": "1.0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Ruelas",
            "email": "gruelas@gruelas.com"
        },
        {
            "name": "Raul Cruz",
            "email": "rcruz@teleurban.mx"
        },
        {
            "name": "Guillermo Corona",
            "email": "gcorona@teleurban.mx"
        }
    ],
    "require": {
        "php": "^8.2",
        "equidna/laravel-toolkit": "^1.0",
        "inertiajs/inertia-laravel": "^1.0"
    },
    "require-dev": {
        "laravel/framework": "^11.21",
        "phpstan/phpstan": "*",
        "squizlabs/php_codesniffer": "^4.0",
        "stevebauman/autodoc-facades": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Equidna\\SwifthAuth\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": []
    },
    "extra": {
        "laravel": {
            "providers": [
                "Equidna\\SwifthAuth\\Providers\\SwiftAuthServiceProvider"
            ]
        }
    },
    "repositories": {},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
